<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="project" package="com.axelor.apps.project.db"/>

    <entity name="Project">

        <many-to-one name="customer" ref="com.axelor.apps.base.db.Partner" title="Client"/>
        <many-to-one name="address" ref="com.axelor.apps.base.db.Address" title="Adresse"/>
        <many-to-one name="sector" ref="com.axelor.apps.timecard.db.Sector" title="Secteur"/>

        <boolean name="isChantier" default="false"/>

    </entity>

</domain-models>
