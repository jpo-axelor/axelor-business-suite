<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <menuitem name="menu-timecard" title="Pointage" icon="fa-list-ul" icon-background="#E0A936"/>


    <menuitem name="timecard-planningline" parent="menu-timecard" title="Lignes de planning" action="timecard.planningline"/>

    <action-view name="timecard.planningline" title="Lignes de planning" model="com.axelor.apps.timecard.db.PlanningLine">
        <view type="grid" name="planning-line-grid"/>
        <view type="form" name="planning-line-form"/>
    </action-view>


    <menuitem name="timecard-frequency" parent="menu-timecard" title="Fréquences" action="timecard.frequency"/>

    <action-view name="timecard.frequency" title="Fréquences" model="com.axelor.apps.timecard.db.Frequency">
        <view type="grid" name="frequency-grid"/>
        <view type="form" name="frequency-form"/>
    </action-view>

</object-views>
