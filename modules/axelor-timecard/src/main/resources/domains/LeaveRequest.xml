<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="human-resource" package="com.axelor.apps.hr.db"/>

    <entity name="LeaveRequest" cachable="true">

        <many-to-many name="projectSet" ref="com.axelor.apps.project.db.Project" title="Projects"/>
        <decimal name="totalAbsenceHours" title="Absence duration (hours)" readonly="true"/>
        <decimal name="totalSubstitutionHours" title="Substitution duration (hours)" readonly="true"/>
        <one-to-many name="timecardLineList" title="Timecard lines" ref="com.axelor.apps.timecard.db.TimecardLine" mappedBy="leaveRequest"/>

    </entity>

</domain-models>
