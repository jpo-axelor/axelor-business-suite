<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="timecard" package="com.axelor.apps.timecard.db"/>

    <entity name="EmployeeSuggestion">

        <many-to-one name="employee" ref="com.axelor.apps.hr.db.Employee" title="Employee"/>
        <boolean name="hasWorkedOnOneOfTheProjects" title="Has worked on one of the projects"/>
        <boolean name="hasMainEmployementContract" title="Has main employement contract"/>
        <decimal name="totalHoursWorked" title="Total hours worked"/>

    </entity>

</domain-models>
