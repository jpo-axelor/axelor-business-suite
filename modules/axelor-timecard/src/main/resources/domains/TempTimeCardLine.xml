<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="timecard" package="com.axelor.apps.timecard.db"/>

    <entity name="TempTimeCardLine">

        <string name="fullName" title="Nom complet"/>

        <many-to-one name="project" title="Projet" ref="com.axelor.apps.project.db.Project"/>
        <many-to-one name="employee" title="Employé" ref="com.axelor.apps.hr.db.Employee" readonly="true"/>
        <datetime name="startDateTime" title="Date et heure de début"/>
        <datetime name="endDateTime" title="Date et heure de fin"/>
        <string name="typeSelect" title="Type de ligne" selection="timecardline.timecard.type.select"/>

        <extra-code>
            <![CDATA[
                // Type Select
                public static final String TYPE_CONTRACTUAL = "contractual";
                public static final String TYPE_EXTRA = "extra";
                public static final String TYPE_ABSENCE = "absence";
            ]]>
        </extra-code>

    </entity>

</domain-models>
