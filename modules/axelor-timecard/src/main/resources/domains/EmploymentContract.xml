<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="human-resource" package="com.axelor.apps.hr.db"/>

    <entity name="EmploymentContract">

        <decimal name="monthlyHours" title="Monthly hours"/>
        <one-to-many name="planningLineList" ref="com.axelor.apps.timecard.db.PlanningLine" title="Planning lines" mappedBy="employmentContract"/>
        <one-to-many name="timecardLineList" ref="com.axelor.apps.timecard.db.TimecardLine" title="Timecard lines" mappedBy="employmentContract"/>

    </entity>

</domain-models>
